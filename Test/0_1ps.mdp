;
;	Last Edited - Sameer - 12.06.09
;
title               =  Slow Growth
cpp                 =  /usr/bin/cpp
;define 		    = -DPOSRES
integrator          =  md
tinit 		    =  0.0
dt                  =  0.002	; ps !
nsteps              =  500	; total 1 ps.
;
; CONSTRAINTS
;
constraints         =  all-bonds
comm_mode	    =  linear
nstcomm             =  100
;
; OUTPUT CONTROL
;
nstxout             =  50
nstvout             =  50
nstlog              =  10
nstenergy           =  10
;energygrps          =  SOL
nstxtcout	    =  50
;
; NEIGHBOR SEARCH & ELECTROSTATICS
;
pbc                 =  xyz
nstlist             =  5
ns_type             =  grid
coulombtype         =  pme
rcoulomb            =  1.0
fourierspacing      =  0.15
pme_order           =  4
ewald_rtol          =  1.E-5
optimize_fft        =  yes
vdwtype		    =  cut-off
rvdw                =  1.0
rlist               =  1.0
;
; Temperature Coupling
;
Tcoupl              =  nose-hoover
nh-chain-length	    =  1
tc_grps  	    =  system
tau_t               =  0.2
ref_t               =  310
;
; ENERGY EXCLUSIONS
;
;energygrp_excl      =  Complex Complex SOL SOL
;
; PRESSURE COUPLING
;
Pcoupl              =  no
;Pcoupltype          =  isotropic
;tau_p               =  1
;compressibility     =  4.5e-5
;ref_p               =  1
;
; GENERATE VELOCITIES
;
gen_vel             =  no
gen_temp 	    =  310
gen_seed	    =  122293
;
; Slow Growth
;
free_energy	    =  no
;init_lambda	    =  0.21
;delta_lambda	    =  0
;sc_alpha	    =  0

